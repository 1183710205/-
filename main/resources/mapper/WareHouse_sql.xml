<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cc.dao.wareHouseDao">
    <insert id="addWareHouse" >
        insert into warehouse(warehousename) values(#{wareHouseName})
    </insert>
    <select id="getAllWareHouse" resultType="String">
        SELECT warehousename FROM warehouse
    </select>
</mapper>